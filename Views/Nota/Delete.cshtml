@model Nota

@{
    ViewData["Title"] = "Deleting Page";
}

<div class="text-center">
    <h1 class="display-4">Remover Nota</h1>
        <div class="mb-3">
        <h1 class="display-4">Quer mesmo remover a Nota? (@Model.Valor)</h1>
        <a type="submit" class="btn btn-danger" asp-controller="Nota" asp-action="Remove" asp-route-id="@Model.NotaID">Remover</a>
        <a type="submit" class="btn btn-primary" asp-controller="Nota" asp-action="Index">Voltar</a>
        </div>
</div>
